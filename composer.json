{
  "name": "test/test-sample-php-app",
  "description": "PHP client for Kount Risk Inquiry Service.",
  "type": "library",
  "license": "proprietary",
  "require": {
    "php": ">=7.2.5",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-xml": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.3",
    "phpunit/php-code-coverage": "^9.2.3",
    "phpdocumentor/phpdocumentor": ">=2.0"
  },
  "autoload": {
    "classmap": [
      "./src/"
    ]
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true
    }
  }
}